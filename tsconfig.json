{
    "compilerOptions": {
        "target": "es2018",
        "module": "commonjs",
        "allowJs": true,
        "outDir": "./dist",
        "rootDir": "./src",
        "strict": true,
        "lib": ["es2018", "esnext.asynciterable"],
        "strictPropertyInitialization": false,
        "esModuleInterop": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "forceConsistentCasingInFileNames": true
    }
}